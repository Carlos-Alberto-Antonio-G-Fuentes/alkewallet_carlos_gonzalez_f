<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profilepage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfilePage7Activity">

    <ImageView
        android:id="@+id/rectangulo"
        android:layout_width="412dp"
        android:layout_height="148dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle" />

    <ImageView
        android:id="@+id/flechaderecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_icon" />

    <TextView
        android:id="@+id/enviarmoney"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="119dp"
        android:fontFamily="@font/jua"
        android:text="@string/textingresardinero25"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/reem2"
        android:layout_width="60sp"
        android:layout_height="60sp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangulo"
        app:srcCompat="@drawable/picture_reem" />

    <TextView
        android:id="@+id/reem3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textoreem47"
        android:fontFamily="@font/jua"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="178dp" />

    <TextView
        android:id="@+id/mailreem"
        android:layout_width="126dp"
        android:layout_height="24dp"
        android:layout_marginTop="1dp"
        android:fontFamily="@font/jua"
        android:text="@string/textomailreem53"
        android:textColor="@color/material_dynamic_neutral80"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/reem3"
        app:layout_constraintTop_toBottomOf="@+id/reem3" />

    <TextView
        android:id="@+id/cantingresar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/jua"
        android:text="@string/textcantingresar28"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reem2" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/ingreem"
        android:layout_width="345sp"
        android:layout_height="56sp"
        android:layout_marginEnd="15dp"
        android:textColorHint="@color/azul"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/cantingresar"
        app:layout_constraintTop_toBottomOf="@+id/cantingresar"
        app:layout_constraintBaseline_creator="">

        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:fontFamily="@font/jua"
            android:hint="20.00"
            android:textColor="@color/azul"
            android:textSize="16sp" />

    </com.google.android.material.textfield.TextInputLayout>


</androidx.constraintlayout.widget.ConstraintLayout>